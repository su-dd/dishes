  "steps": [
    {
      "description": "青茄子、青辣椒、西红柿、洋葱、大葱洗净",
      "time_unit": "分钟",
      "time": [
        2,
        5
      ],
      "time_type": "take",
      "work_stage": "prepare"
    },
    {
      "description": "大葱切5毫米宽的葱花，大蒜扒皮并拍碎，西红柿切6立方厘米的块，青辣椒、洋葱切5g的块",
      "time_unit": "分钟",
      "time": [
        5,
        8
      ],
      "time_type": "take",
      "work_stage": "deal"
    },
    {
      "description": "茄子切菱形块（先切2公分厚的片，然后再把片切成2公分的条，最后斜刀切块儿）",
      "time_unit": "分钟",
      "time": [
        3,
        5
      ],
      "time_type": "take",
      "work_stage": "deal"
    },
    {
      "description": "将面粉倒入盆中，依次加入少量水，搅拌均匀，呈粘稠糊状",
      "time_unit": "分钟",
      "time": [
        2,
        3
      ],
      "time_type": "take",
      "work_stage": "prepare"
    },
    {
      "description": "加入淀粉，加入30克水，搅拌均匀",
      "time_unit": "分钟",
      "time": [
        1,
        2
      ],
      "time_type": "take",
      "work_stage": "prepare"
    },
    {
      "description": "将鸡蛋打到盆中，加入盐，搅拌均匀",
      "time_unit": "分钟",
      "time": [
        1,
        2
      ],
      "time_type": "take",
      "work_stage": "prepare"
    },
    {
      "description": "将茄块倒入面糊中，搅拌使茄块的每一面都能沾上面糊",
      "time_unit": "分钟",
      "time": [
        2,
        3
      ],
      "time_type": "take",
      "work_stage": "deal"
    },
    {
      "description": "开大火，热锅，加入500毫升的油，当能看到锅里的油冒出一丝烟时，调至小火",
      "time_unit": "分钟",
      "time": [
        2,
        3
      ],
      "time_type": "take",
      "work_stage": "cook"
    },
    {
      "description": "将茄块用筷子夹入到油锅，待所有的茄块下锅之后，调至中火，直到茄块变金黄色时捞出",
      "time_unit": "分钟",
      "time": [
        5,
        8
      ],
      "time_type": "take",
      "work_stage": "cook"
    },
    {
      "description": "将油倒出",
      "time_unit": "秒",
      "time": [
        10,
        20
      ],
      "time_type": "take",
      "work_stage": "cook"
    },
    {
      "description": "加入油，放入大蒜、葱花，翻炒15秒",
      "time_unit": "秒",
      "time": [
        15,
        15
      ],
      "time_type": "take",
      "work_stage": "cook"
    },
    {
      "description": "放入青辣椒块翻炒30秒",
      "time_unit": "秒",
      "time": [
        30,
        30
      ],
      "time_type": "take",
      "work_stage": "cook"
    },
    {
      "description": "放入西红柿翻炒30秒",
      "time_unit": "秒",
      "time": [
        30,
        30
      ],
      "time_type": "take",
      "work_stage": "cook"
    },
    {
      "description": "放入炸好的茄块，加水面高度为锅内食材的0.8倍",
      "time_unit": "秒",
      "time": [
        20,
        30
      ],
      "time_type": "take",
      "work_stage": "cook"
    },
    {
      "description": "放入酱油和盐",
      "time_unit": "秒",
      "time": [
        10,
        15
      ],
      "time_type": "take",
      "work_stage": "cook"
    },
    {
      "description": "等待，直到汤汁呈粘稠状（水位大概为剩余食材高度的0.2-0.3倍）",
      "time_unit": "分钟",
      "time": [
        8,
        12
      ],
      "time_type": "wait",
      "work_stage": "cook"
    },
    {
      "description": "开盖，盛出菜，关火",
      "time_unit": "秒",
      "time": [
        30,
        45
      ],
      "time_type": "take",
      "work_stage": "other"
    }
  ],
